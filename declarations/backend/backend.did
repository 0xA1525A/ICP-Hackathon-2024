type Iden = text;
type AccountData = 
 record {
   balance: float64;
   firstname: text;
   isSuspended: bool;
   lastname: text;
   password: text;
 };
service : {
  createAccount: (text, text, text, text, float64) -> (vec Iden);
  getAllIden: () -> (vec Iden);
  getBalance: (text) -> (float64);
  hashPassword: (text) -> (text);
  transfer: (Iden, Iden, float64) -> (vec AccountData);
  validateLogin: (text, text) -> (bool);
}
